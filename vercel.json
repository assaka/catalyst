{
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "https://catalyst-backend-fzhu.onrender.com/api/$1"
    },
    {
      "source": "/public/:storeSlug/robots.txt",
      "destination": "https://catalyst-backend-fzhu.onrender.com/public/$storeSlug/robots.txt"
    },
    {
      "source": "/robots.txt",
      "destination": "https://catalyst-backend-fzhu.onrender.com/robots.txt"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/assets/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}